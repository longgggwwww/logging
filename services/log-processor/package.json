{
  "name": "log-processor",
  "version": "1.0.0",
  "type": "module",
  "description": "Kafka consumer service to process error logs and store to PostgreSQL using Prisma",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "fix:all": "npm run lint:fix && npm run format"
  },
  "keywords": [
    "kafka",
    "consumer",
    "prisma",
    "postgresql",
    "logging"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/redis": "^4.0.10",
    "kafkajs": "^2.2.4",
    "mongoose": "^7.4.8",
    "redis": "^5.9.0"
  },
  "devDependencies": {
    "@types/kafkajs": "^1.8.2",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
