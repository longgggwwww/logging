{
  "name": "api",
  "version": "1.0.0",
  "description": "API service for querying logs with Redis cache",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "fix:all": "npm run lint:fix && npm run format"
  },
  "keywords": [
    "api",
    "express",
    "redis",
    "prisma",
    "logs"
  ],
  "author": "DANH PHI LONG + AI",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "keycloak-connect": "^22.0.3",
    "mongoose": "^7.4.8",
    "redis": "^5.9.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21"
  }
}
