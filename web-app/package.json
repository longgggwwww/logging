{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "repository": "git@github.com:ant-design/ant-design-pro.git",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "biome:lint": "npx @biomejs/biome lint",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "npm run biome:lint && npm run tsc",
    "lint-staged": "lint-staged",
    "openapi": "max openapi",
    "prepare": "husky",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development UMI_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env UMI_ENV=dev MOCK=none max dev",
    "start:no-mock": "cross-env MOCK=none max dev",
    "start:pre": "cross-env UMI_ENV=pre MOCK=none max dev",
    "start:test": "cross-env UMI_ENV=test MOCK=none max dev",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/pro-components": "^2.8.9",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "antd": "^5.26.4",
    "antd-style": "^3.7.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "keycloak-js": "^26.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.1.0",
    "@biomejs/biome": "^2.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@umijs/max": "^4.0.7",
    "cross-env": "^10.0.0",
    "express": "^4.21.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "underscore": "^1.13.7",
    "immer": "^10.1.1",
    "axios": "^1.7.9",
    "form-data": "^4.0.1",
    "body-parser": "^1.20.3",
    "qs": "^6.13.0",
    "node-fetch": "^2.7.0",
    "extend2": "^1.0.1",
    "@babel/runtime": "^7.26.10",
    "postcss": "^8.4.49",
    "tough-cookie": "^5.0.0",
    "path-to-regexp": "^8.2.0",
    "send": "^0.19.1",
    "cross-spawn": "^7.0.6",
    "debug": "^4.4.0",
    "dicer": "^0.3.1",
    "marked": "^15.0.4",
    "busboy": "^1.6.0"
  }
}
